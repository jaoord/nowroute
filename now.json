{
    "version": 2,
    "name": "pocnowroute",
    "builds": [
        {
            "src": "next.config.js",
            "use": "@now/next"
        }
    ],
    "build": {
        "env": {
            "LANGUAGE": "nl-NL"
        }
    },
    "env": {
        "LANGUAGE": "nl-NL"
    },
    "routes": [
        {
            "src": "/diensten",
            "dest": "services"
        },
        {
            "src": "/diensten/(?<slug>[^/]+)",
            "dest": "services/[slug].js?slug=$slug"
        }
    ]
}